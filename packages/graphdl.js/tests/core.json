{
  "_id": "graphdl",
  "_name": "GraphDL",
  "_icon": "ðŸœ‰",
  "_description": "GraphDL is a graph-based data language for describing data models and data relationships.",
  "import": {
    "schema": "https://schema.org/version/latest/schemaorg-current-https.jsonld",
    "epcis": "https://raw.githubusercontent.com/gs1/EPCIS/master/JSON-LD-Context/epcis-context-root.jsonld"
  },
  "nouns": {
    "Graph": {
      "_id": "graph",
      "_name": "Graph",
      "_icon": "âˆµ",
      "_properties": [
        {
          "name": "_description",
          "type": "A [Graph] consists of the [Noun]s in it's domain space."
        },
        {
          "name": "nouns",
          "type": "Noun"
        }
      ]
    },
    "Noun": {
      "_id": "noun",
      "_name": "Noun",
      "_icon": "â– ",
      "_properties": [
        {
          "name": "_id",
          "type": "string",
          "defaultValue": "slugify(_name) ?? base62(8)"
        },
        {
          "name": "_icon",
          "types": [
            {
              "type": "string"
            },
            {
              "type": "url"
            }
          ],
          "defaultValue": "â– "
        },
        {
          "name": "_name",
          "type": "string",
          "defaultValue": "titleCase(_id)"
        },
        {
          "name": "_type",
          "type": "string",
          "defaultValue": "Noun"
        },
        {
          "name": "_plural",
          "type": "string",
          "defaultValue": "pluralize(_name)"
        },
        {
          "name": "_sameAs",
          "type": "url",
          "defaultValue": "https://schema.org/Thing"
        },
        {
          "name": "_description",
          "types": [
            {
              "type": "markdown"
            },
            {
              "type": "html"
            }
          ],
          "defaultValue": "A [Noun] is a person, place, thing, or idea."
        },
        {
          "name": "_source",
          "types": [
            {
              "type": "url"
            },
            {
              "type": "string",
              "many": true
            },
            {
              "type": "object",
              "many": true
            }
          ]
        },
        {
          "name": "_seed",
          "types": [
            {
              "type": "url"
            },
            {
              "type": "string",
              "many": true
            },
            {
              "type": "object",
              "many": true
            }
          ]
        },
        {
          "name": "_visibility",
          "type": "string",
          "options": [
            "anonymous",
            "public",
            "tenant",
            "user",
            "admin"
          ],
          "defaultValue": "tenant"
        },
        {
          "name": "_graph",
          "type": "Graph.nouns"
        },
        {
          "name": "_properties",
          "type": "Property"
        }
      ]
    },
    "Property": {
      "_id": "property",
      "_name": "Property",
      "_icon": "âˆ·",
      "_properties": [
        {
          "name": "id",
          "type": "string!",
          "required": true
        },
        {
          "name": "name",
          "type": "string",
          "defaultValue": "titleCase(id)"
        },
        {
          "name": "icon",
          "types": [
            {
              "type": "string"
            },
            {
              "type": "url"
            },
            {
              "type": "svg"
            }
          ]
        },
        {
          "name": "description",
          "types": [
            {
              "type": "string"
            },
            {
              "type": "markdown"
            }
          ]
        },
        {
          "name": "type",
          "type": "string",
          "options": [
            "string",
            "markdown",
            "url",
            "email",
            "phone",
            "date",
            "time",
            "datetime",
            "timestamp",
            "number",
            "integer",
            "currency",
            "boolean",
            "object"
          ]
        }
      ]
    }
  }
}